(def-symbol (delay ex) (lambda () ex))
(def (force de:function) (de))
(def (curry f:function n:int)
    (def (curry-rec i:int l:list)
        (if (= i 0)
            (apply-list f l)
            (lambda (a)
                (curry-rec (- i 1) (list-add l a)))))
    (curry-rec n (list)))
(def-variadic (maybe f:function v:any)
    (def flag true)
    (for-each (lambda (e) (if (= e nil) (set! flag false))) v)
    (if (flag) (apply-list f v) nil))
